type Player {
    id: ID!,
    name: String,
    team: Team
}
enum Team {
    CSK
    MI
    DC
    SRH
    KKR
    KXIP
    RCB
    RR
    LSG
    GT
}

type Mutation {
    create(name: String, team: Team): Player,
    update(id: ID, name: String, team: Team): Player,
}

type Query {
    findAll: [Player],
    findOne(id: ID): Player
}